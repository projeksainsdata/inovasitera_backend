{
  "name": "inovasiitera.backend",
  "version": "1.0.0",
  "description": "REST API for inovasiitera",
  "main": "./src/app.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-server": "babel ./src --out-dir ./build --ignore ./src/__tests__",
    "build": "npm run clean && npm run build-server",
    "start": "pm2 start ./build/app.js -i 3  --no-daemon",
    "dev": "nodemon --exec babel-node ./src/app.js",
    "monit": "pm2 monit",
    "stop": "pm2 stop all",
    "format": "prettier --write .",
    "lint": "eslint . ",
    "lint:fix": "eslint . --fix ",
    "test": "cross-env NODE_ENV=test jest --testTimeout=1000",
    "prepare": "husky"
  },
  "keywords": [
    "REST",
    "API",
    "inovasi",
    "Indonesia",
    "ITERA"
  ],
  "author": "EXSTR",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@eslint/js": "^9.10.0",
    "@types/express": "^4.17.17",
    "@types/node": "^22.5.5",
    "@types/supertest": "^6.0.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.6.3",
    "chai": "^5.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.10.0",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "mongoose": "^8.6.3",
    "mongoose-mock": "^0.4.0",
    "nodemon": "^3.0.1",
    "pm2": "^5.3.0",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.662.0",
    "@aws-sdk/s3-request-presigner": "^3.662.0",
    "@babel/cli": "^7.12.17",
    "@babel/node": "^7.12.17",
    "@babel/runtime": "^7.12.18",
    "@godaddy/terminus": "^4.6.0",
    "aws-sdk": "^2.1691.0",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mcache": "^0.4.0",
    "memory-cache": "^0.2.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.15",
    "rate-limiter-flexible": "^5.0.3",
    "winston": "^3.10.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run format",
      "npm run lint:fix",
      "npm run lint",
      "git add ."
    ]
  }
}
